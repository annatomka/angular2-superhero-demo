{
  "rules": {
    "curly": true,
    "eofline": false,
    "align": [true, "parameters"],
    "class-name": true,
    "indent": [true, "spaces"],
    "max-line-length": [true, 150],
    "no-consecutive-blank-lines": true,
    "no-trailing-whitespace": true,
    "no-duplicate-key": true,
    "no-duplicate-variable": true,
    "no-unreachable": true,
    "no-var-keyword": true,
    "one-line": [true,
      "check-else",
      "check-whitespace",
      "check-open-brace"],
    "quotemark": [true,
      "single",
      "avoid-escape"],
    "semicolon": true,
    "typedef-whitespace": [true, {
      "call-signature": "nospace",
      "index-signature": "nospace",
      "parameter": "nospace",
      "property-declaration": "nospace",
      "variable-declaration": "nospace"
    }],
    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"]
  }
}